<div class="all-event">
  <h2>"<%= @event.name %>" created by "<%= @event.user.username %>"</h2>
  <div class="past-events">
    <h4>Event Attendees</h4>
    <table border="1">
      <thead>
      <tr>
        <th>Username</th>
        <th>Email</th>
      </tr>
      </thead>
        <tbody>
        <% @event.event_users.each do |c| %>
        <tr>
          <td><%= c.username %> </td>
          <td><%= c.email %></td>
        </tr>
          <%end %>
        </tbody>
    </table>
  </div>
</div>

